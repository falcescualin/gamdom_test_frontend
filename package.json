{
  "name": "@minimal-kit/vite-ts",
  "author": "Minimals",
  "version": "5.7.0",
  "description": "Vite & TypeScript",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "tsc && vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "dev:host": "vite --host",
    "test": "jest --passWithNoTests --silent --ci --coverage",
    "test:watch": "jest --watch --coverage",
    "sources": "./generate-sources.sh"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@hookform/resolvers": "^3.3.2",
    "@iconify/react": "^4.1.1",
    "@tanstack/react-query": "^5.32.1",
    "axios": "^1.6.2",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "history": "^5.3.0",
    "i18next": "^23.7.8",
    "i18next-browser-languagedetector": "^7.2.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "qs": "^6.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.3",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "swr": "^2.2.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.32.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.4",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "postcss": "^8.4.44",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.1.2",
    "typescript": "5.3.3",
    "vite": "^5.0.7",
